pipeline {
    agent any
    
    tools{
        jdk 'jdk11'
        maven 'maven3'
    }

    stages {
        stage('Git check out') {
            steps {
                git branch: 'feature-2', url: 'https://github.com/jaiswaladi246/Petclinic.git'
            }
        }
        
        stage('Maven compile') {
            steps {
                sh 'mvn clean compile'
            }
        }
        
        stage('Maven package') {
            steps {
                sh 'mvn clean package -DskipTest=true'
            }
        }
        
    }
    
}
